{
  "name": "chance-generators",
  "description": "Random generators based on changejs",
  "author": "Sune Simonsen",
  "license": "MIT",
  "version": "2.13.1",
  "main": "lib/chance-generators.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel --out-dir lib --quiet src",
    "coverage": "istanbul cover node_modules/.bin/_mocha",
    "generate-site": "generate-site --require ./bootstrap-unexpected-markdown.js",
    "lint-staged": "lint-staged",
    "prepare": "npm run build",
    "test": "mocha ./documentation/**/*.md ./test/**/*.js",
    "update-examples": "generate-site --require ./bootstrap-unexpected-markdown.js --update-examples"
  },
  "dependencies": {
    "chance": "1.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "es6-set": "0.1.5",
    "eslint": "3.19.0",
    "eslint-config-pretty-standard": "0.2.2",
    "istanbul": "0.4.2",
    "javascript-state-machine": "2.3.5",
    "lint-staged": "4.0.3",
    "mocha": "3.5.3",
    "pre-commit": "1.2.2",
    "prettier": "1.10.1",
    "prettier-package-json": "1.4.0",
    "unexpected": "10.33.2",
    "unexpected-documentation-site-generator": "4.2.0",
    "unexpected-markdown": "1.6.1"
  },
  "keywords": [
    "random",
    "testing"
  ],
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint",
        "prettier --write",
        "git add"
      ],
      "package.json": [
        "prettier-package-json --write",
        "git add"
      ]
    }
  },
  "pre-commit": "lint-staged"
}
